    "@smui/select": "^6.0.0-beta.16",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/dom-serial": "^1.0.2",
    "events": "^3.3.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-kit": "^1.0.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
